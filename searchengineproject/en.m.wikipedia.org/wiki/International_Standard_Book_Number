        <span class="kw1">if</span><span class="br0">(</span><span class="kw3">strlen</span><span class="br0">(</span><span class="re0">$ISBN10</span><span class="br0">)</span> <span class="sy0">!=</span> <span class="nu0">10</span><span class="br0">)</span>

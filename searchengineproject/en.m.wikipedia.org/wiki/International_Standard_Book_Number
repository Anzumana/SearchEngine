      check <span class="sy0">+=</span> <span class="nu0">3</span> <span class="sy0">*</span> <span class="sy0">+</span>ISBNumber<span class="br0">[</span>i<span class="br0">]</span><span class="sy0">;</span>
    <span class="br0">}</span>
    <span class="kw1">return</span> check <span class="sy0">%</span> <span class="nu0">10</span> <span class="sy0">===</span> <span class="nu0">0</span><span class="sy0">;</span>

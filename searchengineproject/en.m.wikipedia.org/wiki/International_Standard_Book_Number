    ISBNumber <span class="sy0">=</span> ISBNumber.<span class="me1">replace</span><span class="br0">(</span><span class="co2">/-\s/g</span><span class="sy0">,</span><span class="st0">''</span><span class="br0">)</span><span class="sy0">;</span>
 

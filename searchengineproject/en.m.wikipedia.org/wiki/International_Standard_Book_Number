<div class="ruby source-ruby">
<pre class="de1">
<span class="co1"># Ruby</span>
<span class="kw1">def</span> isbn_checksum<span class="br0">(</span>isbn_string<span class="br0">)</span>
  digits = isbn_string.<span class="kw3">split</span><span class="br0">(</span><span class="sy0">//</span><span class="br0">)</span>.<span class="me1">map</span><span class="br0">(</span><span class="sy0">&amp;</span>:to_i<span class="br0">)</span>
  transformed_digits = digits.<span class="me1">each_with_index</span>.<span class="me1">map</span> <span class="kw1">do</span> <span class="sy0">|</span>digit, digit_index<span class="sy0">|</span>
    digit_index.<span class="me1">modulo</span><span class="br0">(</span><span class="nu0">2</span><span class="br0">)</span>.<span class="me1">zero</span>? ? digit : digit<span class="sy0">*</span><span class="nu0">3</span>
  <span class="kw1">end</span>
  sum = transformed_digits.<span class="me1">reduce</span><span class="br0">(</span>:<span class="sy0">+</span><span class="br0">)</span>
<span class="kw1">end</span>
 
<span class="kw1">def</span> is_valid_isbn13?<span class="br0">(</span>isbn13<span class="br0">)</span>
  checksum = isbn_checksum<span class="br0">(</span>isbn13<span class="br0">)</span>
  checksum.<span class="me1">modulo</span><span class="br0">(</span><span class="nu0">10</span><span class="br0">)</span>.<span class="me1">zero</span>?
<span class="kw1">end</span>
 
<span class="kw1">def</span> isbn13_checksum_digit<span class="br0">(</span>isbn12<span class="br0">)</span>
  checksum = isbn_checksum<span class="br0">(</span>isbn12<span class="br0">)</span>
  <span class="nu0">10</span> <span class="sy0">-</span> checksum.<span class="me1">modulo</span><span class="br0">(</span><span class="nu0">10</span><span class="br0">)</span>
<span class="kw1">end</span>
</pre>
